{
  "name": "vue-ts-tpl",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "serve": "vite preview",
    "lint": "eslint --config .eslintrc.js",
    "lint:css": "stylelint src/**/*.{vue,css,sass,scss}",
