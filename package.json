    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5.4.4",
    "unocss": "^0.59.0",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.10"
