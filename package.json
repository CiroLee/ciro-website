    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "stylelint": "^15.9.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
